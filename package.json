{
	"name": "regimen-homepage",
	"description": "Chinese medicine based cosmetic product home page",
	"license": "MIT",
	"scripts": {
		"develop": "gatsby develop --https",
		"start": "npm run develop",
		"build": "rm -rf public && gatsby build",
		"build:netlify": "sed -i \"s/branch: master/branch: ${CMS_BRANCH}/g\" static/admin/config.yml",
		"serve": "gatsby serve",
		"prod": "gatsby build && gatsby serve",
		"lint": "eslint src/ --ext .js --ext .jsx",
		"deploy:surge": "yarn run build && surge ./public/ --domain https://regimen.surge.sh"
	},
	"dependencies": {
		"antd": "3.13.2",
		"classnames": "2.2.6",
		"enquire-js": "0.2.1",
		"eslint": "5.13.0",
		"gatsby": "^2.0.33",
		"gatsby-image": "2.0.29",
		"google-maps-react": "2.0.2",
		"lodash-es": "4.17.11",
		"netlify-cms": "2.4.1",
		"rc-drawer": "1.7.7",
		"react": "^16.5.1",
		"react-anchor-link-smooth-scroll": "1.0.11",
		"react-dom": "^16.5.1",
		"react-helmet": "5.2.0",
		"react-inview-monitor": "2.2.0",
		"react-messenger-customer-chat": "0.7.2",
		"react-pose": "4.0.7"
	},
	"devDependencies": {
		"eslint-config-airbnb": "17.1.0",
		"eslint-plugin-import": "2.16.0",
		"eslint-plugin-jsx-a11y": "6.2.1",
		"eslint-plugin-react": "7.12.4",
		"babel-eslint": "10.0.1",
		"gatsby-cli": "2.4.6",
		"gatsby-plugin-antd": "2.0.2",
		"gatsby-plugin-less": "2.0.11",
		"gatsby-plugin-manifest": "2.0.18",
		"gatsby-plugin-sass": "2.0.7",
		"gatsby-plugin-netlify": "2.0.9",
		"gatsby-plugin-netlify-cms": "3.0.12",
		"gatsby-plugin-polyfill-io": "1.0.5",
		"gatsby-plugin-react-helmet": "3.0.6",
		"gatsby-plugin-sharp": "2.0.20",
		"gatsby-remark-copy-linked-files": "2.0.9",
		"gatsby-remark-images": "3.0.3",
		"gatsby-remark-relative-images": "0.2.1",
		"gatsby-source-filesystem": "2.0.20",
		"gatsby-transformer-remark": "2.2.4",
		"gatsby-transformer-sharp": "2.1.13",
		"http-server": "0.11.1",
		"less": "3.9.0",
		"node-sass": "4.11.0",
		"path": "0.12.7"
	}
}
